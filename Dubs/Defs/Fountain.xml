<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--장식-->
  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>TobesFountain</defName>
    <label>Fountain</label>
    <thingClass>DubsBadHygiene.Building_basin</thingClass>
    <graphicData>
      <texPath>Furniture/WaterFountain</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2.2,2.2)</drawSize>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>200</costStuffCount>
    <statBases>
      <MaxHitPoints>220</MaxHitPoints>
      <WorkToBuild>30000</WorkToBuild>
      <Mass>18</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>120</Beauty>
    </statBases>
    <size>(2,2)</size>
    <description>A fountain. Looking good.</description>
    <castEdgeShadows>true</castEdgeShadows>
    <pathCost>60</pathCost>
    <fillPercent>0.4</fillPercent>
    <socialPropernessMatters>true</socialPropernessMatters>
    <designationHotKey>Misc8</designationHotKey>
    <comps>
      <li>
        <compClass>CompGatherSpot</compClass>
      </li>
      <li Class="DubsBadHygiene.CompProperties_Pipe">
        <mode>Sewage</mode>
      </li>
      <li Class="CompProperties_Forbiddable" />
    </comps>
  </ThingDef>
</Defs>
