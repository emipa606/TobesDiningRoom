  <!--의자-->
  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>T_Sofa</defName>
    <label>Small Sofa</label>
    <graphicData>
      <texPath>Furniture/Sofa</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1.1,1.1)</drawSize>
      <shadowData>
        <volume>(0.6, 0.6, 0.6)</volume>
      </shadowData>
      <damageData>
        <rectN>(0,0,1,0.703125)</rectN>
        <rectS>(0.09375,0,0.84375,1)</rectS>
        <rectE>(0,0,0.84375,0.625)</rectE>
        <rectW>(0.15625,0,0.84375,0.625)</rectW>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>220</MaxHitPoints>
      <WorkToBuild>7500</WorkToBuild>
      <Mass>18</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>1</Beauty>
      <Comfort>0.8</Comfort>
    </statBases>
    <description>A large, soft, comfy seat. Great for relaxation.</description>
    <socialPropernessMatters>true</socialPropernessMatters>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <pathCost>30</pathCost>
    <fillPercent>0.40</fillPercent>
    <designationHotKey>Misc8</designationHotKey>
    <building>
      <isSittable>true</isSittable>
    </building>
  </ThingDef>
